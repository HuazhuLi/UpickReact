  "scripts": {
    "deploy:remove": "rm -rf ./.build",
    "deploy:init": "git clone -b release https://github.com/farawaaay/Upick.git ./.build",
    "deploy": "rm -rf ./.build/dist/* && npm run build && cp -rf ./dist/* ./.build/dist && cd ./.build && git add . && git commit -m 'auto deploy commit' && git push origin release",
    "dev": "NODE_ENV=development node ./build/dev.js",
    "build": "NODE_ENV=production node ./build/build.js",
    "stats": "webpack --config=./config/webpack.config.prod.js --profile --json > ./stats.json",
    "analyze": "webpack-bundle-analyzer ./stats.json"
  },
  "config": {
    "deployToGit": {
        "repository": "git@github.com:farawaaay/Upick.git",
        "branch": "release",
        "folder": ".build",
        "script": "npm run build && cp -rf ./dist ./.build",
        "commit": "Automatic commit text",
        "user": {
            "email": "hzylovelyl@gmail.com",
            "name": "Faraway"
        }
    }
